{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/",
      "dest": "/src/static/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/src/static/$1"
    }
  ]
}
