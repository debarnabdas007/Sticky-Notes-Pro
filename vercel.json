{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "src/static/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/health",
      "destination": "/main.py"
    },
    {
      "source": "/api/(.*)",
      "destination": "/main.py"
    },
    {
      "source": "/",
      "destination": "/src/static/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/src/static/$1"
    }
  ]
}
